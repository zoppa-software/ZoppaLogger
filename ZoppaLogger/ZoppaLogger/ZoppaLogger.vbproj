<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>ZoppaLogger</RootNamespace>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<VersionPrefix>1.0.1</VersionPrefix>
	<Authors>zoppa software</Authors>
	<Company />
	<PackageProjectUrl>https://github.com/zoppa-software/ZoppaLogger</PackageProjectUrl>
	<PackageReadmeFile>NugetReadme.md</PackageReadmeFile>
	<RepositoryUrl>https://github.com/zoppa-software/ZoppaLogger</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<Copyright>Copyright © 2023 zoppa software</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\NugetReadme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
